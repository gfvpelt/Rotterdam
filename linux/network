 cd /etc/systemd/network
 touch 10-1.WAN.network
 touch 10-2.LAN.network
 touch 20-1.BRIDGE0.netdev
 touch 30-1.BRIDGE0.network
 vi 10-1.WAN.network
 vi 10-2.LAN.network
 vi 20-1.BRIDGE0.netdev
 vi 30-1.BRIDGE0.network
 shutdown -r +15
 apt-get remove ifup ufw
